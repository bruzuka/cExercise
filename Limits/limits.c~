#include <stdio.h>
#include <limits.h>
#include <float.h>

#define NAME(var) #var

int main() {

	printf("CHAR_BIT = %d\n"
			"SCHAR_MAX = %d\n"
			"CHAR_MIX = %d\n"
			"INT_MAX = %d\n"
			"INT_MIN = %d\n"
			"LONG_MAX = %ld\n"
			"LONG_MIN = %ld\n"
			"SCHAR_MAX = %d\n"
			"SCHAR_MIN = %d\n"
			"SHRT_MAX = %d\n"
			"SHRT_MIN = %d\n"
			"UCHAR_MAX = %d\n"
			"UINT_MAX = %d\n"
			"ULONG_MAX = %ld\n"
			"USHRT_MAX = %d\n"
			"FLT_MAX = %f\n"
			"DBL_MAX = %f\n",\
		   	CHAR_BIT,\
		   	SCHAR_MAX,\
		   	CHAR_MIN,\
		   	INT_MAX,\
		   	INT_MIN,\
		   	LONG_MAX,\
		   	LONG_MIN,\
		   	SCHAR_MAX,\
			SCHAR_MIN,\
		   	SHRT_MAX,\
		   	SHRT_MIN,\
		   	UCHAR_MAX,\
		   	UINT_MAX,\
		   	ULONG_MAX,\
		   	USHRT_MAX,\
			FLT_MAX,\
			DBL_MAX\
			);
	
	return 0;
}

